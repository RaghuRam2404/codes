#include <bits/stdc++.h>
using namespace std;

class FoxAndWord{
	public:
	int howManyPairs(vector<string> in){
		int n = in.size(), ans=0;
		string a,b;
		for (int i=0; i<n-1; i++){
			a = in[i];
			for(int j=i+1; j<n; j++){
				b = in[j];
				// compare a,b
				
				if(a.size() == b.size()){
					for(int k=1; k<=n-1; k++){
						// first size is from 0 to k
						// and k+1 to n
						string a1 = a.substr(0,k);
						string a2 = a.substr(k,n-k);
						if(a1+a2 == b || a2+a1 == b){
							ans ++; break;
						}
					}
				}
				
			}
		}
		return ans;
	}
};


int main(){
	vector<string> in;
	
}
